<Window x:Class="CSHP320_Assignment4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSHP320_Assignment4"
        mc:Ignorable="d"
        Title="Homework 4" Height="260" Width="280">
    <StackPanel>

        <TextBlock>Enter A Valid Zip Code:</TextBlock>
        <TextBox x:Name="ZipCode" TextChanged="ZipCode_TextChanged"></TextBox>
        <Button x:Name="ZipSubmit" IsEnabled="False" Click="ZipSubmit_Click">Submit</Button>


    </StackPanel>
</Window>
